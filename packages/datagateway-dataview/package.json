{
  "name": "datagateway-dataview",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "6.4.3",
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@mui/icons-material": "5.5.0",
    "@mui/material": "5.5.0",
    "@types/history": "4.7.11",
    "@types/jest": "27.5.2",
    "@types/jsrsasign": "9.0.3",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.memoize": "4.1.6",
    "@types/node": "17.0.17",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.3",
    "@types/react-virtualized": "9.21.10",
    "@types/redux-logger": "3.0.8",
    "axios": "0.27.2",
    "connected-react-router": "6.9.1",
    "custom-event-polyfill": "1.0.7",
    "datagateway-common": "^1.1.0",
    "date-fns": "2.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "3.4.1",
    "history": "4.10.1",
    "i18next": "21.6.13",
    "i18next-browser-languagedetector": "6.1.2",
    "i18next-http-backend": "1.4.1",
    "jsrsasign": "10.5.25",
    "lodash.debounce": "4.0.8",
    "lodash.memoize": "4.1.2",
    "loglevel": "1.8.0",
    "prettier": "2.2.1",
    "react": "17.0.2",
    "react-app-polyfill": "3.0.0",
    "react-dom": "17.0.2",
    "react-i18next": "11.15.4",
    "react-query": "3.18.1",
    "react-redux": "7.2.6",
    "react-router-dom": "5.3.0",
    "react-scripts": "5.0.0",
    "react-virtualized": "9.22.3",
    "redux": "4.1.2",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.4",
    "redux-thunk": "2.4.1",
    "single-spa-react": "4.6.1",
    "tslib": "2.3.1",
    "typescript": "4.5.3",
    "url-search-params-polyfill": "8.1.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "serve:build": "yarn build && serve -l 5001 build",
    "test": "craco test --env=jsdom --coverage --watchAll=false",
    "test:watch": "craco test --env=jsdom --watch",
    "build:e2e": "cross-env REACT_APP_E2E_TESTING=true GENERATE_SOURCEMAP=false craco build",
    "e2e:serve": "yarn build:e2e && node ./server/e2e-test-server.js",
    "e2e:interactive": "start-server-and-test e2e:serve http://localhost:3000 cy:open",
    "e2e": "start-server-and-test e2e:serve http://localhost:3000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "lint:js": "eslint --ext=tsx --ext=ts --ext=js --ext=jsx --fix ./src && yarn build",
    "eject": "react-scripts eject",
    "pre-commit": "lint-staged"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{tsx,ts,js,jsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/setupTests.ts",
      "!src/**/*.d.ts",
      "!src/state/app.types.tsx"
    ],
    "resetMocks": false
  },
  "lint-staged": {
    "src/**/*.{tsx,ts,js,jsx,json}": [
      "eslint --max-warnings=0 --ext=tsx --ext=ts --ext=js --ext=jsx  --fix",
      "prettier --write"
    ],
    "cypress/**/*.{tsx,ts,js,jsx}": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "12.1.3",
    "@types/enzyme": "3.10.10",
    "@types/react-redux": "7.1.22",
    "@types/redux-mock-store": "1.0.3",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "@welldone-software/why-did-you-render": "6.2.3",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.6",
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.3",
    "cypress": "9.7.0",
    "cypress-failed-log": "2.9.1",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.1",
    "eslint": "8.19.0",
    "eslint-config-react-app": "7.0.0",
    "express": "4.18.1",
    "lint-staged": "10.5.4",
    "serve": "13.0.2",
    "start-server-and-test": "1.14.0"
  }
}
