    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitest/coverage-v8": "3.2.3",
    "cross-env": "7.0.3",
    "cypress": "13.17.0",
    "eslint": "8.57.1",
    "eslint-config-react-app": "7.0.0",
    "express": "4.21.2",
    "jsdom": "26.1.0",
    "lint-staged": "13.3.0",
    "start-server-and-test": "~2.0.11",
    "vitest": "3.2.3",
    "vitest-fail-on-console": "0.7.1"
  }
