{
  "name": "datagateway-search",
  "version": "2.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@date-io/date-fns": "2.17.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.0",
    "@mui/x-date-pickers": "6.11.2",
    "@tanstack/react-query": "4.36.1",
    "@tanstack/react-query-devtools": "4.36.1",
    "@types/history": "4.7.11",
    "@types/jest": "29.5.2",
    "@types/jsrsasign": "10.5.2",
    "@types/lodash.isequal": "4.5.8",
    "@types/node": "20.11.5",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@types/react-router-dom": "5.3.3",
    "@types/react-virtualized": "9.21.10",
    "@types/redux-logger": "3.0.8",
    "@vitejs/plugin-react": "4.3.4",
    "axios": "1.8.3",
    "browserslist": "4.24.4",
    "browserslist-to-esbuild": "2.1.1",
    "datagateway-common": "^2.1.0",
    "date-fns": "2.30.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-prettier": "4.2.1",
    "history": "4.10.1",
    "i18next": "22.0.3",
    "i18next-browser-languagedetector": "7.2.0",
    "i18next-http-backend": "2.4.2",
    "jsrsasign": "11.0.0",
    "lodash.isequal": "4.5.0",
    "loglevel": "1.9.1",
    "prettier": "2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.3.1",
    "react-redux": "8.1.3",
    "react-router-dom": "5.3.4",
    "react-virtualized": "9.22.5",
    "redux": "4.2.0",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.4",
    "redux-thunk": "2.4.1",
    "single-spa-react": "5.0.1",
    "tslib": "2.6.0",
    "typescript": "5.3.3",
    "vite": "6.2.1"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "preview:build": "yarn build && yarn preview",
    "preview:build:dev": "yarn build --watch & yarn preview",
    "test": "vitest --coverage",
    "build:e2e": "cross-env VITE_BUILD_STANDALONE=true GENERATE_SOURCEMAP=false vite build",
    "e2e:serve": "yarn build:e2e && node ./server/e2e-test-server.js",
    "e2e:interactive": "start-server-and-test e2e:serve http://localhost:3000 cy:open",
    "e2e": "start-server-and-test e2e:serve http://localhost:3000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "lint:js": "eslint --max-warnings=0 --ext=tsx --ext=ts --ext=js --ext=jsx --fix ./src ./cypress && tsc --noEmit",
    "eject": "react-scripts eject",
    "pre-commit": "lint-staged"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|copy-anything|is-what))"
    ],
    "collectCoverageFrom": [
      "src/**/*.{tsx,js,jsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/setupTests.js"
    ],
    "resetMocks": false
  },
  "lint-staged": {
    "src/**/*.{tsx,ts,js,jsx,json}": [
      "eslint --max-warnings=0 --ext=tsx --ext=ts --ext=js --ext=jsx  --fix",
      "prettier --write"
    ],
    "cypress/**/*.{tsx,ts,js,jsx}": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.23.3",
    "@testing-library/cypress": "10.0.0",
    "@testing-library/jest-dom": "6.4.1",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/react-redux": "7.1.22",
    "@types/redux-mock-store": "1.0.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitest/coverage-v8": "3.0.9",
    "cross-env": "7.0.3",
    "cypress": "13.6.4",
    "cypress-failed-log": "2.10.0",
    "eslint": "8.56.0",
    "eslint-config-react-app": "7.0.0",
    "express": "4.20.0",
    "jsdom": "26.0.0",
    "lint-staged": "13.3.0",
    "start-server-and-test": "~2.0.11",
    "vitest": "3.0.9",
    "vitest-fail-on-console": "0.7.1"
  }
}
